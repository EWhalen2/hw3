<h1><%= @place["name"] %></h1>
<p>
<a href="/posts/new?place_id=<%=@place["id"]%>">New Post</a>
</p>

<p>
<ul>
    <% for post in @posts %>
    <li>
        <%= post["title"] %>,
        <%= post["description"]%>,
        <%= post["posted_on"]%>
    </li>
    <% end %>
    </ul>
</p>

    <p>
        <a href="/places"> Back to all places </a>
    </p>
