<h1><%= @place["name"] %></h1>
<p>
<a href="/posts/new?place_id=<%=@place["id"]%>">New Post</a>
</p>

<ul>
    <% for post in @posts %>
    <li>
        <%= post["title"] %>
        <%= post["description"]%>
        <%= post["date"]%>
    </li>
    <%end%>
    
    </ul>
    <p>
        <a href="/places"> Back to all places </a>
    </p>
